<?xml version="1.0"?>

<jetscape>
  
  <nEvents> 10 </nEvents>
  
  <JetScapeWriterHepMC> on </JetScapeWriterHepMC>
  <JetScapeWriterAscii> on </JetScapeWriterAscii>

  <Random>
    <seed>10</seed>
  </Random>

  <!-- Hard Process -->
  <Hard>
    <PythiaGun>
      <pTHatMin>5</pTHatMin>
      <pTHatMax>7</pTHatMax>
      <eCM>200</eCM>
    </PythiaGun>
  </Hard>
  
  <!--Eloss Modules -->
  <Eloss>
    <deltaT> 0.1 </deltaT>
    <maxT> 20 </maxT>
    <Matter> 
      <Q0> 1.0 </Q0>
      <in_vac> 1 </in_vac>
      <vir_factor> 0.25 </vir_factor>
      <qhat0> 0.0 </qhat0>
      <alphas> 0.25 </alphas>      
    </Matter>
  </Eloss>
  
  <!-- Jet Hadronization Module -->
  <JetHadronization>
    <name>colored</name>
  </JetHadronization>
  
</jetscape>
