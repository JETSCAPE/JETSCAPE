    </Brick>
  </Hydro>


  
  
  <Eloss>
    <maxT> 0.1 </maxT>
    <ModificationFactor> 6.24 </ModificationFactor> 
   <!--Matter>
      <name>Matter</name>
      <matter_on> 1 </matter_on>
      <Q0> 2.0 </Q0>
      <T0> 0.16 </T0>
      <initial_virtuality_pT> 1 </initial_virtuality_pT>
