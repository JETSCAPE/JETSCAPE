    <Lbt>
      <name> Lbt </name>
      <Q0> 2.0 </Q0>
      <in_vac> 0 </in_vac>
      <only_leading> 0 </only_leading>
      <hydro_Tc> 0.16 </hydro_Tc>
      <alphas> 0.2 </alphas>
      <run_alphas>1</run_alphas>   
    <filename>Gluon</filename> <!-- File name for the gluon distribution, used in the Lbt module -->
    <enter_temp>0.3</enter_temp>
    <enter_pid> 21 </enter_pid>
    </Lbt>

  </Eloss>
  
  
  <JetHadronization>
    <name>colorless</name>
    <take_recoil>1</take_recoil>
    <eCMforHadronization>2510</eCMforHadronization>
    <LinesToRead>
      111:mayDecay = off
    </LinesToRead>
  </JetHadronization>
