<?xml version="1.0"?>

<jetscape>
  
  <nEvents> 10 </nEvents>
  
  <outputFilename>/scratch/user/cameron.parker/projects/JETSCAPE/test/test</outputFilename>
  <JetScapeWriterAscii> on </JetScapeWriterAscii>
  
  <!-- Hard Process -->
  <Hard>
    <PythiaGun>
      <pTHatMin>0</pTHatMin>
      <pTHatMax>1000</pTHatMax>
      <eCM>2760</eCM>
      <LinesToRead>
         MultipartonInteractions:pT0Ref = 2.28
         MultipartonInteractions:ecmPow = 0.215
      </LinesToRead>
      <FSR_on>1</FSR_on>
    </PythiaGun>
  </Hard>
  
  <!--Eloss Modules -->
  <Eloss>
    <maxT>200</maxT>
    <Matter> 
      <matter_on>0</matter_on>
      <Q0> 1.0 </Q0>
      <in_vac> 1 </in_vac>
      <vir_factor> 0.25 </vir_factor>
      <recoil_on> 0 </recoil_on>
      <broadening_on> 0 </broadening_on>
      <brick_med> 0 </brick_med>
    </Matter>
  </Eloss>
  
  <!-- Jet Hadronization Module -->
  <JetHadronization>
    <name>hybrid</name>
  </JetHadronization>
  
</jetscape>
