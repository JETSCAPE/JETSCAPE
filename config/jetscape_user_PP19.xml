<?xml version="1.0"?>

<jetscape>
  
  <nEvents> 1000 </nEvents>
  
  <outputFilename>test_out</outputFilename>
  <JetScapeWriterHepMC> on </JetScapeWriterHepMC>
  
  <!-- Hard Process -->
  <Hard>
    <PythiaGun>
      <pTHatMin>235</pTHatMin>
      <pTHatMax>1000</pTHatMax>
      <eCM>5020</eCM>
    </PythiaGun>
  </Hard>
  
  <!--Eloss Modules -->
  <Eloss>
    <Matter> 
      <Q0> 1.0 </Q0>
      <in_vac> 1 </in_vac>
      <vir_factor> 0.25 </vir_factor>
      <recoil_on> 0 </recoil_on>
      <broadening_on> 0 </broadening_on>
      <brick_med> 0 </brick_med>
    </Matter>
  </Eloss>
  
  <!-- Jet Hadronization Module -->
  <JetHadronization>
    <name>colorless</name>
  </JetHadronization>
  
</jetscape>
