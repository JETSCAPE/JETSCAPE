<?xml version="1.0"?>

<jetscape>
  
  <nEvents> 40000 </nEvents>
  
  <outputFilename>/scratch/user/cameron.parker/projects/JETSCAPE/test</outputFilename>
  <JetScapeWriterAsciiGZ> on </JetScapeWriterAsciiGZ>
  <finalHadsOnly> on </finalHadsOnly>
  
  <!-- Hard Process -->
  <Hard>
    <PythiaGun>
      <pTHatMin>20</pTHatMin>
      <pTHatMax>22.5</pTHatMax>
      <eCM>200</eCM>
      <LinesToRead>
         MultipartonInteractions:pT0Ref = 2.28
         MultipartonInteractions:ecmPow = 0.215
      </LinesToRead>
      <softMomentumCutoff>0.0</softMomentumCutoff>
    </PythiaGun>
  </Hard>
  
  <!--Eloss Modules -->
  <Eloss>
    <maxT> 100 </maxT>
    <lambdaQCD> 0.4 </lambdaQCD>
    
    <Matter> 
      <Q0>1</Q0>
      <QS> 0.9 </QS>
      <in_vac> 1 </in_vac>
      <vir_factor>0.25</vir_factor>
      <recoil_on> 0 </recoil_on>
      <broadening_on> 0 </broadening_on>
      <brick_med> 0 </brick_med>
    </Matter>
  </Eloss>
  
  <!-- Jet Hadronization Module -->
  <JetHadronization>
    <name>hybrid</name>
    <eCMforHadronization>200</eCMforHadronization>
    <shower_recofactor>1.0</shower_recofactor>
    <LinesToRead>
      StringFlav:probStoUD = 0.217
      StringFlav:probQQtoQ = 0.081
      ColourReconnection:reconnect = on
      ColourReconnection:mode = 2
    </LinesToRead>
    <tau0Max>30.0</tau0Max>
    <part_prop>0.0</part_prop>
  </JetHadronization>
  
</jetscape>
