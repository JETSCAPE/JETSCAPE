<?xml version="1.0"?>

<jetscape>
  
  <nEvents> 100000 </nEvents>
  
  <outputFilename>/scratch/user/cameron.parker/projects/JETSCAPE/test/test</outputFilename>
  <JetScapeWriterAscii> on </JetScapeWriterAscii>
  <finalHadsOnly> on </finalHadsOnly>

  <IDs>
    411
    421
  </IDs>
  
  <!-- Hard Process -->
  <Hard>
    <PythiaGun>
      <pTHatMin>0</pTHatMin>
      <pTHatMax>5</pTHatMax>
      <eCM>5020</eCM>
      <LinesToRead>
        HardQCD:all = off
        SoftQCD:inelastic = on
      </LinesToRead>
    </PythiaGun>
  </Hard>
  
  <!--Eloss Modules -->
  <Eloss>
    <maxT> 200 </maxT>
    <lambdaQCD> 0.2 </lambdaQCD>
    
    <Matter> 
      <Q0>1</Q0>
      <in_vac> 1 </in_vac>
      <vir_factor>0.25</vir_factor>
      <recoil_on> 0 </recoil_on>
      <broadening_on> 0 </broadening_on>
      <brick_med> 0 </brick_med>
    </Matter>
  </Eloss>
  
  <!-- Jet Hadronization Module -->
  <JetHadronization>
    <name>hybrid</name>
    <eCMforHadronization>5020</eCMforHadronization>
    <shower_recofactor>1</shower_recofactor>
    <thermal_recofactor>1</thermal_recofactor>
    <ignorePhotons>on</ignorePhotons>
    <part_prop>0.0</part_prop>
    <LinesToRead>
      StringFlav:probStoUD = 0.217
      StringFlav:probQQtoQ = 0.081
      ParticleDecays:tau0Max = 10
      411:mayDecay = off
      -411:mayDecay = off
      421:mayDecay = off
      -421:mayDecay = off
      ColourReconnection:reconnect = on
      ColourReconnection:mode = 1
      310:onMode=off
      3112:onMode=off
      3122:onMode=off
      3222:onMode=off
      3312:onMode=off
      3322:onMode=off
      3334:onMode=off
    </LinesToRead>
  </JetHadronization>
  
</jetscape>
