<?xml version="1.0"?>

<jetscape>
  
  <nEvents> 100 </nEvents>
  
  <outputFilename>/scratch/user/cameron.parker/JETSCAPE-COMP-HH_colorrecomb/test</outputFilename>
  <JetScapeWriterAscii> on </JetScapeWriterAscii>
  
  <!-- Hard Process -->
  <Hard>
    <PythiaGun>
      <pTHatMin>0</pTHatMin>
      <pTHatMax>5</pTHatMax>
      <eCM>5020</eCM>
      <LinesToRead>
        HardQCD:all = off
        HardQCD:hardccbar = on
      </LinesToRead>
    </PythiaGun>
  </Hard>
  
  <!--Eloss Modules -->
  <Eloss>
    <maxT> 200 </maxT>
    <lambdaQCD> 0.2 </lambdaQCD>
    
    <Matter> 
      <Q0>1</Q0>
      <in_vac> 1 </in_vac>
      <vir_factor>0.25</vir_factor>
      <recoil_on> 0 </recoil_on>
      <broadening_on> 0 </broadening_on>
      <brick_med> 0 </brick_med>
    </Matter>
  </Eloss>
  
  <!-- Jet Hadronization Module -->
  <JetHadronization>
    <name>hybrid</name>
    <eCMforHadronization>5020</eCMforHadronization>
    <pionWidthScale>1.0</pionWidthScale>
    <kaonWidthScale>1.0</kaonWidthScale>
    <protonWidthScale>1.0</protonWidthScale>
    <LinesToRead>
      StringFlav:probStoUD = 0.217
      StringFlav:probQQtoQ = 0.081
      ParticleDecays:tau0Max = 10
      413:mayDecay = off
      -413:mayDecay = off
      411:mayDecay = off
      -411:mayDecay = off
      421:mayDecay = off
      -421:mayDecay = off
    </LinesToRead>
  </JetHadronization>
  
</jetscape>
