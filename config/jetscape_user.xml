<?xml version="1.0"?>

<jetscape>
  
  <nEvents> 2 </nEvents>
  
  <JetScapeWriterAscii> on </JetScapeWriterAscii>

  <!-- Inital State Module  -->
  <IS>
    <Trento> </Trento>
  </IS>
  
  <!-- Hard Process -->
  <Hard>
    <!-- epem Gun -->
    <!-- Sensible defaults are set in the class. ptHat etc. get their own field -->
    <epemGun>
      <name>epemGun</name>
      <FSR_on>0</FSR_on>
      <pTHatMin>100</pTHatMin>
      <pTHatMax>120</pTHatMax>
      <eCM>91.19</eCM>
      <!-- You can add any number of additional lines to initialize pythia here -->
      <!-- Note that if the tag exists it cannot be empty (tinyxml produces a segfault) -->
      <!-- <LinesToRead> -->
      <!--      HardQCD:all = on -->
      <!-- </LinesToRead> -->
    </epemGun>


  </Hard>
  
  <!--Preequilibrium Dynamics Module -->
  <Preequilibrium>
    <NullPreDynamics> </NullPreDynamics>
  </Preequilibrium>
  
  <!-- Hydro  Module  -->
  <Hydro>
    <Brick bjorken_expansion_on="false" start_time="0.6"> </Brick>
  </Hydro>
  
  <!--Eloss Modules -->
  <Eloss>
    <Matter>
      <in_vac> 0 </in_vac>
    </Matter>
    <Lbt> </Lbt>
  </Eloss>
  
  <!-- Jet Hadronization Module -->
  <JetHadronization>
       <name>hybrid</name>
       <take_recoil>1</take_recoil>
       <eCMforHadronization>0</eCMforHadronization>
       <reco_Elevelmax>1</reco_Elevelmax>
       <thermreco_distmax>5.0</thermreco_distmax>
       <shower_recofactor>1.0</shower_recofactor>
       <thermal_recofactor>1.0</thermal_recofactor>
       <weak_decays>on</weak_decays>
   </JetHadronization>


  
</jetscape>
